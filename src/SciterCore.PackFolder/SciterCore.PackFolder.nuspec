<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>SciterCore.PackFolder</id>
    <version>$version$</version>
    <title>SciterCore.PackFolder</title>
    <authors>William D Cossey</authors>
    <owners>wdcossey</owners>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <license type="expression">GPL-3.0-or-later</license>
    <projectUrl>https://github.com/wdcossey/SciterCore.PackFolder</projectUrl>
    <description>SciterCore PackFolder tasks for embedding Sciter resource files.</description>
    <copyright>© Sciter.</copyright>
    <tags>Sciter SciterCore PackFolder</tags>
    <icon>images\icon.png</icon>
    <dependencies>
      <group targetFramework="net45"/>
      <group targetFramework="net5.0"/>
      <group targetFramework="netstandard2.0"/>
      <group targetFramework="netstandard2.1"/>
      <group targetFramework="netcoreapp2.0"/>
      <group targetFramework="netcoreapp3.1"/>
      <group targetFramework="xamarinmac20"/>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\images\sciter_nuget_icon.png" target="images\icon.png" />
    <file src="lib\_._" target="lib/net45/_._" />
    <file src="lib\_._" target="lib/net5.0/_._" />
    <file src="lib\_._" target="lib/netstandard2.0/_._" />
    <file src="lib\_._" target="lib/netstandard2.1/_._" />
    <file src="lib\_._" target="lib/netcoreapp2.0/_._" />
    <file src="lib\_._" target="lib/netcoreapp3.1/_._" />
    <file src="lib\_._" target="lib/xamarinmac20/_._" />
    <file src="build\**\*.props" target="build\net45" />
    <file src="build\**\*.props" target="build\net5.0" />
    <file src="build\**\*.props" target="build\netstandard2.0" />
    <file src="build\**\*.props" target="build\netstandard2.1" />
    <file src="build\**\*.props" target="build\netcoreapp2.0" />
    <file src="build\**\*.props" target="build\netcoreapp3.1" />
    <file src="build\**\*.props" target="build\xamarinmac20" />
    <file src="buildMultiTargeting\net45\*.props" target="buildMultiTargeting\net45" />
    <file src="buildMultiTargeting\net5.0\*.props" target="buildMultiTargeting\net5.0" />
    <file src="buildMultiTargeting\netstandard2.0\*.props" target="buildMultiTargeting\netstandard2.0" />
    <file src="buildMultiTargeting\netstandard2.1\*.props" target="buildMultiTargeting\netstandard2.1" />
    <file src="buildMultiTargeting\netcoreapp2.0\*.props" target="buildMultiTargeting\netcoreapp2.0" />
    <file src="buildMultiTargeting\netcoreapp3.1\*.props" target="buildMultiTargeting\netcoreapp3.1" />
    <file src="buildMultiTargeting\xamarinmac20\*.props" target="buildMultiTargeting\xamarinmac20" />
    <file src="..\..\sciter-sdk\bin.win\packfolder.exe" target="scripts\win\packfolder.exe" />
    <file src="..\..\sciter-sdk\bin.osx\packfolder" target="scripts\osx\packfolder" />
    <file src="..\..\sciter-sdk\bin.lnx\packfolder" target="scripts\lnx\packfolder" />
  </files>
</package>
